<jqassistant-plugin xmlns="http://schema.jqassistant.org/plugin/v2.4"
                    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                    xsi:schemaLocation="http://schema.jqassistant.org/plugin/v2.4 https://jqassistant.github.io/jqassistant/current/schema/jqassistant-plugin-v2.4.xsd"
                    name="jQAssistant Context Mapper Plugin" id="jqa.plugin.contextmapper"
                    version="${project.version}">
    <description>${project.description}</description>
    <model>
        <class>org.jqassistant.plugin.contextmapper.model.BoundedContextBaseDescriptor</class>
        <class>org.jqassistant.plugin.contextmapper.model.BoundedContextDefinesDependency</class>
        <class>org.jqassistant.plugin.contextmapper.model.BoundedContextDependencyDescriptor</class>
        <class>org.jqassistant.plugin.contextmapper.model.BoundedContextDependsOn</class>
        <class>org.jqassistant.plugin.contextmapper.model.BoundedContextDescriptor</class>
        <class>org.jqassistant.plugin.contextmapper.model.ContextMapDescriptor</class>
        <class>org.jqassistant.plugin.contextmapper.model.ContextMapperDescriptor</class>
        <class>org.jqassistant.plugin.contextmapper.model.ContextMapperFileDescriptor</class>
        <class>org.jqassistant.plugin.contextmapper.model.DomainDescriptor</class>
        <class>org.jqassistant.plugin.contextmapper.model.SubdomainDescriptor</class>
    </model>
    <scanner>
        <class>org.jqassistant.plugin.contextmapper.ContextMapperScannerPlugin</class>
    </scanner>
    <rules>
        <resource>context-mapper-plugin/index.adoc</resource>
    </rules>
    <report>
        <class id="context-mapper-diagram">org.jqassistant.plugin.contextmapper.ContextMapperReportPlugin</class>
    </report>
</jqassistant-plugin>